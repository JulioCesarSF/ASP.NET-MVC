@model List<Leilao.Dominio.Models.Negociacao>

@foreach (var item in Model)
{
    <tr>
        <td>@item.Id</td>        
        <td>@Html.ActionLink("Produto", "VisualizarProduto", "Compra", new { Id = item.IdProduto }, new { @class="btn btn-primary btn-sm" })</td>
        <td>
            <button type="button" onclick="idNegociacao.value = @item.Id" class="btn btn-success" data-toggle="modal" data-target="#ModalComprar">
                Ofertar
            </button>
        </td>
    </tr>
}